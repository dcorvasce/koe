{
  "name": "koe",
  "version": "0.1.0",
  "description": "A simple news reader wrote in Python.",
  "main": "assets/index.js",
  "scripts": {
    "build-js": "parcel build assets/javascripts/app.js --out-dir static",
    "build-css": "parcel build assets/stylesheets/style.css --out-dir static",
    "build": "npm run build-js && npm run build-css",
    "watch-js": "parcel watch assets/javascripts/app.js --out-dir static",
